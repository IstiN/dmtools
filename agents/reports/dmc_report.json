{
  "name": "ReportGenerator",
  "params": {
    "reportName": "My Code Contribution Report",
    "startDate": "2024-03-01",
    "employees": [
    ],
    "aliases": {
      "Uladzimir Klyshevich": ["IstiN", "uladzimir-klyshevich-epam"],
      "Yauheni Navakolski": ["ajaxspace", "yauheni-navakolski-epam"],
      "AI Teammate": ["ai-teammate","github-actions[bot]","copilot", "cursor[bot]","copilot-pull-request-reviewer[bot]"]
    },
    "dataSources": [
      {
        "name": "tracker",
        "params": {
          "jql": "project = DMC AND issueType in (Story, Task) and status in (Done)"
        },
        "metrics": [
          {
            "name": "TicketMovedToStatusRule",
            "params": {
              "statuses": ["Done"],
              "label": "Accepted Tickets",
              "isWeight": true
            }
          },
          {
            "name": "TicketMovedToStatusRule",
            "params": {
              "statuses": ["In Review"],
              "label": "Task Completed",
              "isWeight": true
            }
          },
          {
            "name": "TicketCreatorsRule",
            "params": {
              "label": "Task Created",
              "isWeight": true
            }
          }
        ]
      },
      {
        "name": "tracker",
        "params": {
          "jql": "project = DMC AND status in (Done) and issueType in (subtask) and (labels in (q) or summary ~ \"\\\\[Q\\\\]\")"
        },
        "metrics": [
          {
            "name": "TicketCreatorsRule",
            "params": {
              "label": "Questions Created",
              "isWeight": true
            }
          }
        ]
      },
      {
        "name": "tracker",
        "params": {
          "jql": "project = DMC AND status in (Irrelevant) and issueType in (subtask) and (labels in (q) or summary ~ \"\\\\[Q\\\\]\")"
        },
        "metrics": [
          {
            "name": "TicketCreatorsRule",
            "params": {
              "label": "Irrelevant Questions",
              "isWeight": true
            }
          }
        ]
      },
      {
        "name": "tracker",
        "params": {
          "jql": "project = DMC AND status in (Done) and issueType in (subtask) and (labels in (sd_core, sd_api, sd_ui, sd) or summary ~ \"\\\\[SD CORE\\\\]\" or summary ~ \"\\\\[SD API\\\\]\" or summary ~ \"\\\\[SD UI\\\\]\" or summary ~ \"\\\\[SD\\\\]\")"
        },
        "metrics": [
          {
            "name": "TicketMovedToStatusRule",
            "params": {
              "statuses": ["In Review"],
              "label": "Solution Completed",
              "isWeight": true
            }
          }
        ]
      },
      {
        "name": "tracker",
        "params": {
          "jql": "project = DMC AND status in (Done) and issueType in (subtask) and (labels in (core, api, ui) or summary ~ \"\\\\[CORE\\\\]\" or summary ~ \"\\\\[API\\\\]\" or summary ~ \"\\\\[UI\\\\]\") and (summary !~ \"\\\\[Q\\\\]\" and summary !~ \"\\\\[SD CORE\\\\]\" and summary !~ \"\\\\[SD API\\\\]\" and summary !~ \"\\\\[SD UI\\\\]\")"
        },
        "metrics": [
          {
            "name": "TicketMovedToStatusRule",
            "params": {
              "statuses": ["In Review"],
              "label": "Dev Completed",
              "isWeight": true
            }
          }
        ]
      },
      {
        "name": "tracker",
        "params": {
          "jql": "project = DMC AND issueType in (Bug)"
        },
        "metrics": [
          {
            "name": "TicketCreatorsRule",
            "params": {
              "label": "Bugs Created",
              "isWeight": true
            }
          }
        ]
      },
      {
        "name": "tracker",
        "params": {
          "jql": "project = DMC AND issueType in ('Test Case')"
        },
        "metrics": [
          {
            "name": "TicketCreatorsRule",
            "params": {
              "label": "Test Cases",
              "isWeight": true
            }
          }
        ]
      },
      {
        "name": "tracker",
        "params": {
          "jql": "project = DMC"
        },
        "metrics": [
          {
            "name": "CommentsWrittenRule",
            "params": {
              "label": "Comments Written",
              "isWeight": true
            }
          },
          {
            "name": "CommentsWrittenRule",
            "params": {
              "label": "Test Case Agent Comments",
              "isWeight": true,
              "commentsRegex": "Test Case Agent - similar test cases are linked and new test cases are generated"
            }
          },
          {
            "name": "TicketFieldsChangesRule",
            "params": {
              "label": "Description Changes",
              "isWeight": true,
              "filterFields": ["description"],
              "includeInitial": true,
              "useDivider": false,
              "creatorFilterMode": "all"
            }
          },
          {
            "name": "TicketFieldsChangesRule",
            "params": {
              "label": "Description Similarity",
              "isWeight": true,
              "isSimilarity": true,
              "filterFields": ["description"],
              "includeInitial": true,
              "creatorFilterMode": "all"
            }
          },
          {
            "name": "TicketFieldsTokensChangedRule",
            "params": {
              "label": "Description Tokens Contribution (K)",
              "divider": 1000,
              "isWeight": true,
              "filterFields": ["description"],
              "includeInitial": true,
              "mode": "contribution",
              "creatorFilterMode": "all"
            }
          },
          {
            "name": "TicketFieldsTokensRetainedRule",
            "params": {
              "label": "Description Tokens Retained (K)",
              "divider": 1000,
              "isWeight": true,
              "filterFields": ["description"],
              "includeInitial": true,
              "creatorFilterMode": "all"
            }
          },
          {
            "name": "TicketFieldsChangesRule",
            "params": {
              "label": "Status Changes",
              "isWeight": true,
              "filterFields": ["status"]
            }
          },
          {
            "name": "TicketFieldsChangesRule",
            "params": {
              "label": "Diagrams Changes",
              "isWeight": true,
              "filterFields": ["diagrams"],
              "includeInitial": true,
              "useDivider": false,
              "creatorFilterMode": "all"
            }
          },
          {
            "name": "TicketFieldsChangesRule",
            "params": {
              "label": "Diagrams Similarity",
              "isWeight": true,
              "isSimilarity": true,
              "filterFields": ["diagrams"],
              "includeInitial": true,
              "creatorFilterMode": "all"
            }
          },
          {
            "name": "TicketFieldsTokensChangedRule",
            "params": {
              "label": "Diagrams Tokens Contribution (K)",
              "isWeight": true,
              "divider": 1000,
              "filterFields": ["diagrams"],
              "includeInitial": true,
              "mode": "contribution",
              "creatorFilterMode": "all"
            }
          }
        ]
      },
      {
        "name": "pullRequests",
        "params": {
          "sourceType": "github",
          "workspace": "IstiN",
          "repository": "dmtools",
          "branch": "main"
        },
        "metrics": [
          {
            "name": "PullRequestsMetricSource",
            "params": {
              "label": "PRs Created",
              "isWeight": false,
              "isPersonalized": true
            }
          },
          {
            "name": "PullRequestsMergedByMetricSource",
            "params": {
